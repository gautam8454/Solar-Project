  {{!-- Quote Started here --}}
  <section class="Quote" id="contact-section">
    <div class="form-container no-right-margin">
      <h2 class="heading-4">Request a Quote</h2>
      <div class="generator-install-form w-form contact-form">
        <form id="wf-form-Generator-Install" name="wf-form-Generator-Install" data-name="Generator Install"
          action="./main" method="post" class="form-grid-vertical">
          <div class="labelled-input">
            <label for="Name-3" class="field-label">Your Name</label>
            <input type="text" class="form-input w-input input" maxlength="256" name="Name" data-name="Name"
              placeholder="" id="Name" required="">
          </div>
          <div class="labelled-input">
            <label for="Email-3" class="field-label">Email</label>
            <input type="email" class="form-input w-input input" maxlength="256" name="Email" data-name="Email"
              placeholder="" id="Email" required="">
          </div>
          <div class="labelled-input">
            <label for="Phone" class="field-label">Phone</label>
            <input type="tel" class="form-input w-input input" maxlength="10" name="Phone" data-name="Phone"
              placeholder="" id="Phone">
          </div>
          <div class="labelled-input">
            <label for="Message" class="field-label Message-label">Message</label>
            {{!-- <input type="text" class="form-input-long w-input" maxlength="256" name="Message" data-name="Message"
              placeholder="" id="Message"> --}}
            <textarea class="Message-textarea form-input-long w-input" maxlength="256" name="Message"
              data-name="Message" placeholder="" id="Message" id="" cols="30" rows="10"></textarea>
          </div>
          <div class="checkBox">
            <div>Select:</div>
            <div class="mb-3 form-check">
              <input type="checkbox" class="form-check-input" id="check1" name="Check1">
              <label class="form-check-label" for="check1">Commercial</label>
            </div>
            <div class="mb-3 form-check">
              <input type="checkbox" class="form-check-input" id="check2" name="Check2">
              <label class="form-check-label" for="check2">Residential</label>
            </div>
            <div class="mb-3 form-check">
              <input type="checkbox" class="form-check-input" id="check3" name="Check3">
              <label class="form-check-label" for="check3">Battery Backup</label>
            </div>
            <div class="button">
              <button class="btn">Submit</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </section>


